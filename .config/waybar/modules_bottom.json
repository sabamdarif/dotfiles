{
  "custom/os_button": {
    "format": "üîç",
    "on-click": "wofi --show drun",
    "tooltip": false
  },
  "wlr/taskbar": {
    "format": "{icon}",
    "icon-size": 42,
    "spacing": 8,
    "on-click-middle": "close",
    "tooltip-format": "{title}",
    "ignore-list": [],
    "on-click": "activate"
  },
  "custom/zen-browser": {
    "exec": "echo 'Browser'",
    "interval": 60,
    "return-type": "plain",
    "on-click": "flatpak run app.zen_browser.zen"
  },
  "custom/files": {
    "exec": "echo 'Files'",
    "interval": 60,
    "return-type": "plain",
    "on-click": "nautilus"
  },
  "custom/app-store": {
    "exec": "echo 'App Store'",
    "interval": 60,
    "return-type": "plain",
    "on-click": "flatpak run io.github.kolunmi.Bazaar"
  },
  "custom/ghostty": {
    "exec": "echo 'Terminal'",
    "interval": 60,
    "return-type": "plain",
    "on-click": "ghostty"
  },
  "custom/telegram": {
    "exec": "echo 'Telegram'",
    "interval": 60,
    "return-type": "plain",
    "on-click": "flatpak run org.telegram.desktop"
  },
  "custom/vesktop": {
    "exec": "echo 'Vesktop'",
    "interval": 60,
    "return-type": "plain",
    "on-click": "flatpak run dev.vencord.Vesktop"
  },
  "custom/gimp": {
    "exec": "echo 'Gimp'",
    "interval": 60,
    "return-type": "plain",
    "on-click": "flatpak run org.gimp.GIMP"
  },
  "custom/bottles": {
    "exec": "echo 'Bottles'",
    "interval": 60,
    "return-type": "plain",
    "on-click": "flatpak run com.usebottles.bottles"
  }
}
